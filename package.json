  "scripts": {
    "start": "next start",
    "build": "next build",
    "test": "vitest run",
    "test:frontend": "vitest run",
    "test:backend": "cd backend && source venv/bin/activate && PYTHONPATH=. pytest -q",
    "test:all": "npm run test:frontend && npm run test:backend",
    "dev": "concurrently --kill-others-on-fail \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "cd backend && venv/bin/python run_server.py",
    "backend": "cd backend && venv/bin/python run_server.py",
    "worker": "cd backend && venv/bin/python -m rq.cli worker profiles --url redis://localhost:6379/0",
    "setup": "./scripts/script.sh",
    "quick": "./scripts/quick-run.sh"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
